{
  "name": "kanashin/laravel-db-clear-command",
  "description": "Laravel artisan command to drop all database tables and rerun the migrations.",
  "keywords": [
    "artisan",
    "command",
    "clear database",
    "drop database",
    "laravel"
  ],
  "homepage": "https://github.com/kanashin/laravel-db-clear-command",
  "license": "MIT",
  "authors": [
    {
      "name": "Abhijit Ghogre",
      "email": "abhijitghogre@gmail.com",
      "homepage": "https://github.com/abhijitghogre",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.0",
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Abhijitghogre\\LaravelDbClearCommand\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Abhijitghogre\\LaravelDbClearCommand\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
